};


int Max(int a,int b){
    if(a > b){return a;}
    else {return b;};
};


float Average(int arr[], int n){
    int i=0;
    int max;
    max=arr[0];
 for (i=1;i<n;i++){
    if(max>arr[i]){//do nothing 
        };
     if(max<arr[i]){
        max=arr[i];
     };

 };
 return max;
};


